<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ants.antsbackground.mapper.feedback.FeedbackMapper">
    <!--将用户进行反馈的信息添加到数据库中-->
    <insert id="insertFeedback" parameterType="com.ants.antsbackground.entity.feedback.Feedback">
        INSERT INTO ants_management_feedback (fbId, userId, fbContent, fbTime,
                                             fbSatisfaction, userMobile, userName)
                                             VALUES(#{fbId}, #{userId},#{fbContent},#{fbTime},
                                                     #{fbSatisfaction},#{userMobile},#{userName})


    </insert>

</mapper>